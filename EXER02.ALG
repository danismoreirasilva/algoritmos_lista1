algoritmo "exer02"
var
    raio, altura, volume: real
inicio
      escreva("Digite o valor do raio: ")
      leia(raio)
      escreva("Digite a altura da lata: ")
      leia(altura)
      
      volume <- pi * raio^2 * altura
      
      escreva("O volume da lata é: ",volume)

fimalgoritmo





